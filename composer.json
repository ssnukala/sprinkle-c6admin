{
    "name": "ssnukala/sprinkle-c6admin",
    "type": "userfrosting-sprinkle",
    "description": "Drop-in replacement administrative module for UserFrosting 6, using sprinkle-crud6",
    "keywords": [
        "php user management",
        "userfrosting",
        "sprinkle",
        "userfrosting-sprinkle",
        "admin-dashboard",
        "admin-panel",
        "php",
        "user-management",
        "crud"
    ],
    "homepage": "https://github.com/ssnukala/sprinkle-c6admin",
    "license": "MIT",
    "authors": [
        {
            "name": "Srinivas Nukala",
            "homepage": "https://srinivasnukala.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "userfrosting/framework": "^6.0",
        "userfrosting/sprinkle-core": "^6.0",
        "userfrosting/sprinkle-account": "^6.0",
        "ssnukala/sprinkle-crud6": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-mockery": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.0",
        "phpunit/phpunit": "^10.5",
        "mockery/mockery": "^1.2",
        "fakerphp/faker": "^1.17",
        "slam/phpstan-extensions": "^6.0",
        "php-mock/php-mock-mockery": "^1.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "UserFrosting\\Sprinkle\\C6Admin\\": "app/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserFrosting\\Sprinkle\\C6Admin\\Tests\\": "app/tests/"
        }
    },
    "scripts": {
        "test": [
            "@test:php",
            "@test:frontend"
        ],
        "test:php": "phpunit",
        "test:frontend": "npm run test",
        "phpcs": "php-cs-fixer fix --dry-run --diff --verbose",
        "phpcs:fix": "php-cs-fixer fix --diff --verbose",
        "phpstan": "phpstan analyze app/src --level 8"
    }
}
