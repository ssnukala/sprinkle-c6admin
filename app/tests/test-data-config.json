          "_roles": ["site-admin"]
        },
        {
          "_comment": "c6admin - C6Admin test user with crud6-admin role (ID 101 for testing)",
          "id": 101,
          "user_name": "c6admin",
          "first_name": "C6",
          "last_name": "Administrator",
          "email": "c6admin@example.com",
          "password": "$2y$10$8pJ7TqVqXqKvGqGqVqXqKeVqXqVqXqVqXqVqXqVqXqVqXqVqXqVqO",
          "flag_enabled": 1,
          "flag_verified": 1,
