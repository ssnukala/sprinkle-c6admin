  "relationships": [
    {
      "description": "Role assignments for test users",
      "pivot_table": "role_users",
      "foreign_key": "user_id",
      "related_key": "role_id",
      "assignments": [
        {
          "_comment": "testadmin -> site-admin",
          "select": "SELECT `users`.`id`, `roles`.`id`\nFROM `users`, `roles`\nWHERE `users`.`user_name` = 'testadmin' AND `roles`.`slug` = 'site-admin'"
        },
        {
          "_comment": "c6admin -> crud6-admin",
          "select": "SELECT `users`.`id`, `roles`.`id`\nFROM `users`, `roles`\nWHERE `users`.`user_name` = 'c6admin' AND `roles`.`slug` = 'crud6-admin'"
        },
        {
          "_comment": "testuser -> user",
          "select": "SELECT `users`.`id`, `roles`.`id`\nFROM `users`, `roles`\nWHERE `users`.`user_name` = 'testuser' AND `roles`.`slug` = 'user'"
        },
        {
          "_comment": "testmoderator -> user",
          "select": "SELECT `users`.`id`, `roles`.`id`\nFROM `users`, `roles`\nWHERE `users`.`user_name` = 'testmoderator' AND `roles`.`slug` = 'user'"
        },
        {
          "_comment": "testmoderator -> crud6-admin",
          "select": "SELECT `users`.`id`, `roles`.`id`\nFROM `users`, `roles`\nWHERE `users`.`user_name` = 'testmoderator' AND `roles`.`slug` = 'crud6-admin'"
        }
      ]
    }
  ],
  "usage": {
    "generate": "php generate-test-data.php > test-data.sql",
    "execute": "mysql -h host -u user -p database < test-data.sql",
    "notes": [
      "Run after UserFrosting 6 migrations and base seeds (Account, CRUD6)",
      "Password for all test users: testpass123",
      "Uses _group_slug for group lookup (generates SELECT with JOIN)",
      "Uses _roles array for documentation (actual assignments in relationships section)"
    ]
  }
}
