        },
        {
          "_comment": "Testers group - for QA team (ID 102 for testing)",
          "id": 102,
          "slug": "testers",
          "name": "Testers",
          "description": "Quality assurance and testing team",
          "icon": "fa fa-bug"
        }
      ]
    },
    {
      "model": "users",
      "description": "Test Users - Users with different roles and permissions for testing",
      "records": [
